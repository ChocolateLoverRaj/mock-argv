{
  "name": "mock-argv",
  "version": "1.0.4",
  "main": "src/index.js",
  "repository": "https://github.com/dword-design/mock-argv",
  "author": "Sebastian Landwehr <info@dword-design.de>",
  "license": "MIT",
  "description": "Temporarily overrides the command line arguments. This is useful for testing.",
  "keywords": [
    "argv",
    "args",
    "arguments",
    "process",
    "test",
    "temporary",
    "tmp",
    "temp",
    "mock",
    "replace",
    "restore"
  ],
  "scripts": {
    "prepare:config-files": "dw-config-files",
    "prepare:readme": "mos",
    "prepare": "npm-run-all prepare:*",
    "pretest": "install-self",
    "test:readme": "mos test",
    "test:unit": "nyc --reporter lcov --reporter text mocha",
    "test": "npm-run-all test:*"
  },
  "mos": {
    "installation": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@dword-design/config-files": "^1.0.3",
    "expect": "^24.9.0",
    "husky": "^3.0.5",
    "install-self": "dword-design/install-self#fork",
    "mocha": "^6.2.2",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1"
  }
}
